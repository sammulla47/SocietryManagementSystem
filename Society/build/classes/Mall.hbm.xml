<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.society.Mall" table="mall">

		<id name="id" type="int" column="Id">
			<generator class="increment" />
		</id>

		<property name="SocietyName">
			<column name="SName" />
		</property>

		<property name="Name">
			<column name="Name" />
		</property>

		<property name="Address">
		    <column name="Address"/>
		</property>
		
		<property name="Pincode">
		<column name="Pincode"/>
		</property>
		
		<property name="City">
		<column name="City"/>
		</property>
		
		<property name="OpenTime">
		<column name="OpenTime"/>
		</property>
		
		<property name="ClosedTime">
		<column name="ClosedTime"/>
		</property>
		
	</class>
</hibernate-mapping>